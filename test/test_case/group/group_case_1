create table t_group_1(id int, name char, score float, age int);
create table t_group_2(id int, name char, score float, age int);
insert into t_group_1 values(1, 'hu', 90.0, 18);
insert into t_group_1 values(2, 'liu', 80.0, 22);
insert into t_group_1 values(3, 'li', 70.0, 18);
insert into t_group_1 values(4, 'we', 100.0, 18);
insert into t_group_1 values(5, 'li', 80.0, 22);
insert into t_group_1 values(6, 'we', 70.0, 18);
insert into t_group_1 values(7, 'aa', 60.0, 18);
insert into t_group_2 values(1, 'hu', 90.0, 18);
insert into t_group_2 values(2, 'liu', 80.0, 22);
insert into t_group_2 values(3, 'li', 70.0, 18);
insert into t_group_2 values(4, 'we', 100.0, 18);
insert into t_group_2 values(5, 'li', 80.0, 22);
insert into t_group_2 values(6, 'we', 70.0, 18);
select t_group_1.id, t_group_1.name, avg(t_group_1.score), avg(t_group_2.age) from t_group_1,t_group_2 where t_group_1.id=t_group_2.id group by t_group_1.id,t_group_1.name;
select t_group_1.id, max(t_group_1.name),min(t_group_1.score),count(t_group_1.age),avg(t_group_1.age) from t_group_1,t_group_2 where t_group_1.id=t_group_2.id group by t_group_1.id;
select id, avg(age) from t_group_1 group by id,score;
select t_group_1.id, max(t_group_1.score) from t_group_1 group by t_group_1.name;
select id, avg(score) from t_group_1 group by id;
select id, avg(score) from t_group_1 where id = 10 group by id;
select t_group_1.id, t_group_1.name, avg(t_group_1.score), avg(t_group_2.age) from t_group_1,t_group_2 where t_group_1.id=10 group by t_group_1.id,t_group_1.name;
select max(id), score from t_group_1 group by name;
drop table t_group_1;
drop table t_group_2;