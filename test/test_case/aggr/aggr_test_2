create table t(id int, name char, price float);
insert into t values(1,'hu',2.2);
insert into t values(2,'ni',3.333);
insert into t values(3,'li',1.1);
insert into t values(4,'we',3.155);
insert into t values(5,'ti',1.0);
insert into t values(6,'aa',-21.244);
select count(*) from t;
select count(id) from t;
select min(id) from t;
select min(name) from t;
select max(id) from t;
select max(name) from t;
select avg(id) from t; 
select avg(price) from t;
select avg(price), max(id), max(name) from t;
select count(*,id) from t;
select count() from t;
select count(a) from t;
drop table t;