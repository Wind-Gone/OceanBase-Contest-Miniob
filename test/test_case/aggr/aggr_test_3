create table t(id int, name char, price float,birthday date);
insert into t values(1,'hu',2.2,'2021-3-4');
insert into t values(2,'ni',3.333,'2020-12-21');
insert into t values(3,'li',1.1,'2019-3-4');
insert into t values(4,'we',3.155,'1999-12-4');
insert into t values(5,'ti',1.0,'1988-2-1');
insert into t values(6,'aa',-21.244,'1987-10-18');
select count(*) from t;
select count(id) from t;
select min(id) from t;
select min(name) from t;
select max(id) from t;
select max(name) from t;
select avg(id) from t; 
select avg(price) from t;
select avg(price), max(id), max(name) from t;
select count(*,id) from t;
select count() from t;
select count(a) from t;
select max(birthday) from t;
select min(t.birthday) from t;
select avg(price), max(id), max(name),min(t.birthday),max(birthday) from t;
drop table t;