create table t1 (a int);
create table t2 (b int);
create table t3 (c char,d int);
insert into t1 values(1);
insert into t1 values(2);
insert into t2 values(1);
insert into t3 values('a',2);
select t2.b,t1.a,t3.c from t1 inner join t2 on t1.a=t2.b inner join t3 on t3.d>t2.b and t3.d>t1.a;
drop table t1;
drop table t2;
drop table t3;