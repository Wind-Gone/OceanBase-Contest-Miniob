create table t7 (a int, b float, c char, d date);
create index c_id on t7 (c);
insert into t7 values(1,1.1,'a','2020-1-3');
insert into t7 values(2,1.2,'b','2019-2-28');
insert into t7 values(3,1.3,'c','2018-3-30');
